'needless'
SUB _74HC595_CONFIGURE(G_CLOCK,G_LATCH,G_OUTPUT,G_RESET,G_SDATA)
    CLOCK = G_CLOCK
    LATCH = G_LATCH
    OUTPUT = G_OUTPUT
    RESET = G_RESET
    SDATA = G_SDATA
    SETPIN CLOCK,DOUT
    SETPIN LATCH,DOUT
    SETPIN OUTPUT,DOUT
    SETPIN RESET,DOUT
    SETPIN SDATA,DOUT
END SUB

SUB SHIFT_CLEAR
    PIN(OUTPUT) = 0
    PIN(RESET) = 0
END SUB

SUB _74HC595_OUTPUT_DISABLE
    PIN(OUTPUT) = 1
    PIN(RESET) = 0
END SUB

SUB _74HC595_OUTPUT_ENABLE
    PIN(OUTPUT) = 0
    PIN(RESET) = 1
END SUB

SUB SHIFT_OUT(BYTE)
    PIN(LATCH) = 0
    PIN(SDATA) = 0
    FOR i = 0 TO 7
        PIN(CLOCK) = 0
        IF BYTE AND &H01 = 1 THEN
            PIN(SDATA) = 1
        ELSE
            PIN(SDATA) = 0
        ENDIF
                BYTE = BYTE/&B10
        PIN(CLOCK) = 1
    NEXT
    PIN(LATCH) = 1
END SUB

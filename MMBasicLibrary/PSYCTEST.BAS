Function GetAnswer$(dummyArg)
  Do
    Char$ = UCase$(Inkey$)
  Loop Until Char$ = "Y" Or Char$ = "N" Or Char$ = "?"

  GetAnswer$ = Char$
End Function

Sub InterpretTheAnswers(dummyArg)

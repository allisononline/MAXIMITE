    ' accumulate score
    I1 = Val(Mid$(ScoreI1$,iIndex,1))
    I2 = Val(Mid$(ScoreI2$,iIndex,1))
    ScoreAmt(I1, I2) = ScoreAmt(I1, I2) + QuestVal
  Next

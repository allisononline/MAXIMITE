  Dim lab1$(3), lab2$(3), tlab1$(3), tlab2$(3)
  Dim stm$(3, 7), estmt$(3, 7), tstmt2$(3)

  InitializeTables

  ' Scores by answer
  ScoreYN$ = "YNYNYNYNYNYYNYYYNNYNYYYYNYYYYYYNYYYYYYYNNYYYNNNNNNYNNNYNNYNN"
  ScoreYN$ = ScoreYN$ + "YYYYYYYYYYNNNYNYYYYYNYNYYYYYYYYYNNYNNYYYYYYNYNYYN"

SUB _74HC595_ENABLE(G_LATCH,G_ENABLE,G_SDATA,G_CLK)
	LATCH = G_LATCH
	ENABLE = G_ENABLE
	SDATA = G_SDATA
	CLK - G_CLK
	SETPIN LATCH,DOUT
	SETPIN ENABLE,DOUT
	SETPIN SDATA,DOUT
	SETPIN CLK,DOUT
	LATCH_STATE = 0
	LATCH_TX(LATCH_STATE)
	PIN(ENABLE) = 0
END SUB

SUB LATCH_TX(BYTE)
	PIN(LATCH) = 0
	PIN(SDATA) = 0
	FOR i = 0 TO 7
		PIN(CLK) = 0
		IF BYTE AND &H01 = 1 THEN
			PIN(SDATA) = 1
		ELSE
			PIN(SDATA) = 0
		ENDIF
		BYTE = BYTE / &B10
		PIN(CLK) = 1
	NEXT
	PIN(LATCH) = 1
END SUB

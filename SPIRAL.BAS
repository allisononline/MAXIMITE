LIBRARY LOAD "PRIMENBR.LIB"
CENTRE.X = MM.HRES/2
CENTRE.Y = MM.VRES/2
POINT.X = 0
POINT.Y = 0
MAGX = 0.01

j = 1
DO UNTIL j > MM.HRES
    IF CHECKPRIME(j) = 1 THEN
        POINT.X = CENTRE.X + (SIN(j)*j*MAGX)
        POINT.Y = CENTRE.Y - (COS(j)*j*MAGX)
        PIXEL(POINT.X,POINT.Y) = CYAN
    ENDIF
    j = j + 1
LOOP
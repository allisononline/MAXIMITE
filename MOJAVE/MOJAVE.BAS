OPENGAME = 0

SUB SETFILEPOS PLACE$
    SEEK #3,1
    STRING$ = ""
    DO UNTIL STRING$ = PLACE$
        INPUT #3,STRING$
    LOOP
END SUB

SUB LOCATION FILENAME$
    IF OPENGAME = 1 THEN
        CLOSE #3
    ENDIF
    OPEN FILENAME$ FOR RANDOM AS #3
    NEWCELL "#CELL00#"
    OPENGAME = 1
END SUB

SUB NEWCELL NUMBER$

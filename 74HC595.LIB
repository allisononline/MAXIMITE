'needless'
SUB 74HC595_CONFIGURE(CLOCK,LATCH,OUTPUT,RESET,SDATA)
    SETPIN CLOCK,DOUT
    SETPIN LATCH,DOUT
    SETPIN OUTPUT,DOUT
    SETPIN RESET,DOUT
    SETPIN SDATA,DOUT
END SUB

SUB SHIFT_CLEAR
    PIN(OUTPUT) = 0
    PIN(RESET) = 0
END SUB

SUB LATCH
    PULSE LATCH,3
END SUB

SUB OUTPUT_DISABLE
    PIN(OUTPUT) = 1
    PIN(RESET) = 0
END SUB

SUB OUTPUT_ENABLE
    PIN(OUTPUT) = 0
    PIN(RESET) = 1
END SUB

SUB SHIFT_OUT(BYTE)
    PIN(OUTPUT) = 0
    PIN(RESET) = 1
    FOR i = 0 TO 7
        IF RIGHT$(BIN$(BYTE), 1) = "1" THEN
            PIN(SDATA) = 1
        ELSE
            PIN(SDATA = 0)
        ENDIF
        BITS = FIX(BITS/2)
        PULSE CLOCK, 3
    NEXT
    PIN(SDATA) = 0
    LATCH
END SUB
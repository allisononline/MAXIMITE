'needless'
SUB _74HC595_CONFIGURE(SRCLK,RCLK,OE,SRCLR,SER)
    CLOCK = SRCLK
    LATCH = RCLK
    OUTPUT = OE
    RESET = SRCLR
    SDATA = SER
    SETPIN CLOCK,DOUT
    SETPIN LATCH,DOUT
    SETPIN OUTPUT,DOUT
    SETPIN RESET,DOUT
    SETPIN SDATA,DOUT
END SUB

SUB SHIFT_CLEAR
    PIN(OUTPUT) = 0
    PIN(RESET) = 0
END SUB

SUB OUTPUT_DISABLE
    PIN(OUTPUT) = 1
    PIN(RESET) = 0
END SUB

SUB OUTPUT_ENABLE
    PIN(OUTPUT) = 0
    PIN(RESET) = 1
END SUB

SUB SHIFT_OUT(BYTE)
    PIN(LATCH) = 0
    PIN(SDATA) = 0
    FOR i = 0 TO 7
        PIN(CLOCK) = 0
        IF RIGHT$(BIN$(BYTE), 1) = "1" THEN
            PIN(SDATA) = 1
        ELSE
            PIN(SDATA) = 0
        ENDIF
                BYTE = FIX(BYTE/2)
        PIN(CLOCK) = 1
    NEXT
    PIN(LATCH) = 1
END SUB

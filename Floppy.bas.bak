#DEFINE "FDREDWC", "3"
#DEFINE "FDINDEX", "5"
#DEFINE "FDMOTE0", "11"
#DEFINE "FDDRVS1", "18"
#DEFINE "FDDRVS0", "19"
#DEFINE "FDMOTE1", "21"
#DEFINE "FDDIR", "23"
#DEFINE "FDSTEP", "27"
#DEFINE "FDWDATA", "28"
#DEFINE "FDWEN", "31"
#DEFINE "FDTRK00", "32"
#DEFINE "FDWPT", "33"
#DEFINE "FDRDATA", "35"
#DEFINE "FDSIDE1", "36"
#DEFINE "FDDSKCH", "38"

Sub fdSetup
    SetPin FDREDWC, DOUT, OC: Pin(FDREDWC) = 0
    SetPin FDMOTE0, DOUT, OC: Pin(FDMOTE0) = 1
    SetPin FDDRVS1, DOUT, OC: Pin(FDDRVS1) = 1
    SetPin FDDRVS0, DOUT, OC: Pin(FDDRVS0) = 0
    SetPin FDMOTE1, DOUT, OC: Pin(FDMOTE1) = 1
    SetPin FDDIR, DOUT, OC: Pin(FDDIR) = 1
    SetPin FDSTEP, DOUT, OC: Pin(FDSTEP) = 1
    SetPin FDWDATA, DOUT, OC: Pin(FDWDATA) = 1
    SetPin FDWEN, DOUT, OC: Pin(FDWEN) = 1
    SetPin FDSIDE1, DOUT, OC: Pin(FDSIDE1) = 0
    SetPin FDINDEX, DIN, PULLDOWN
    SetPin FDTRK00, DIN, PULLDOWN

    SetPin FDWPT, DIN, PULLDOWN

    SetPin FDRDATA, DIN, PULLDOWN

    SetPin FDDSKCH, DIN, PULLDOWN

End Sub

fdSetup
Pause 200
Print Pin(FDINDEX), Pin(FDTRK00), Pin(FDWPT), Pin(FDRDATA), Pin(FDDSKCH)
